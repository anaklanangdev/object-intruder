{
    "name": "anaklanangdev/object-intruder",
    "type": "library",
    "description": "A small library to access private/protected properties/methods of objects",
    "keywords": ["reflection", "private", "protected", "public"],
    "homepage": "https://github.com/anaklanangdev/object-intruder",
    "license": "Apache-2.0",
    "authors": [{
        "name": "Mas Ranto",
        "email": "anaklanangnet@gmail.com",
        "homepage": "https://github.com/anaklanangdev",
        "role": "Developer"
    }]
    "require": {
    "anaklanangdev/object-intruder":"dev-master@dev"
  },
  "repositories": [
    {
       "type": "library",
       "url": "https://github.com/anaklanangdev/object-intruder",
    }
  ]
    "require": {
        "php": "^5.6|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6"
    },
    "autoload": {
        "psr-4": {"anaklanangdev\\ObjectIntruder\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"anaklanangdev\\ObjectIntruderTests\\": "tests/"}
    }
}
